{
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-jsy": "^0.9.4",
    "chokidar": "^1.7.0",
    "chokidar-cli": "^1.2.0",
    "fs-extra": "^4.0.3",
    "nodemon": "*"
  },
  "babel": {
    "presets": "jsy/lean"
  },
  "scripts": {
    "start": "node dist",
    "build": "babel -s inline -x .js,.jsy code -d dist",
    "watch": "npm -s run build -- --watch",
    "dkr-watch": "nodemon -d 1 -w ./dist/ --exec 'bash dkr-watch.sh'",
    "pretest": "npm -s run build",
    "test": true
  }
}
